{
  "name": "api",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/api",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx-go/nx-go:build",
      "options": {
        "main": "{projectRoot}/*.go"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
      "cwd": "apps/api",
        "command": "air"
      }
    },
    "test": {
      "executor": "@nx-go/nx-go:test"
    },
    "lint": {
      "executor": "@nx-go/nx-go:lint"
    },
    "tidy": {
      "executor": "@nx-go/nx-go:tidy"
    }
  }
}
